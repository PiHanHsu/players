<ul>
  <% @all_players.each do |player| %>
  <li>
  	<%= player.username%>
    <%= player.first_name %>
    <%= link_to 'Show', player_path(player) %>
    <%= link_to 'Edit', edit_player_path(player) %>
    <%= link_to 'Delete', player_path(player), :method => :delete %>
    
  </li>
  <% end %>
</ul>
<%= link_to 'Add New Player', new_player_path %>
<%= paginate @all_players %>